<Page x:Class="RiSC.DeveloperPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RiSC" xmlns:mainpagemvvm="clr-namespace:RiSC.MainPageMVVM"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DeveloperPage">

    <Grid Background="Transparent" >
        <Border CornerRadius="10" Background="Green">
            <TextBox Margin="5" Text="{Binding Path=DeveloperInfo, UpdateSourceTrigger=PropertyChanged}">
                
            </TextBox>
        </Border>
    </Grid>
</Page>
